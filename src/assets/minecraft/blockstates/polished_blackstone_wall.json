{
    "multipart": [
        {   "when": { "up": "true" },
            "apply": { "model": "block/polished_blackstone_wall_post" }
        },
        {   "when": { "north": "low" },
            "apply": { "model": "block/polished_blackstone_wall_side", "uvlock": true }
        },
        {   "when": { "east": "low" },
            "apply": { "model": "block/walls/polished_blackstone_wall_side_ew", "uvlock": true }
        },
        {   "when": { "south": "low" },
            "apply": { "model": "block/polished_blackstone_wall_side", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "low" },
            "apply": { "model": "block/walls/polished_blackstone_wall_side_ew", "y": 180, "uvlock": true }
        },
        {   "when": { "north": "tall" },
            "apply": { "model": "block/polished_blackstone_wall_side_tall", "uvlock": true }
        },
        {   "when": { "east": "tall" },
            "apply": { "model": "block/walls/polished_blackstone_wall_side_tall_ew", "uvlock": true }
        },
        {   "when": { "south": "tall" },
            "apply": { "model": "block/polished_blackstone_wall_side_tall", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "tall" },
            "apply": { "model": "block/walls/polished_blackstone_wall_side_tall_ew", "y": 180, "uvlock": true }
        }
    ]
}
